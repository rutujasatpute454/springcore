<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	
	<bean class="com.jspiders.springcore.bean.PersonBean" name="Person1">
	<property name="id" value="1"/>
	<property name="name" value="Rutuja"/>
	<property name="aadhar" ref="aadhar1"/>

	</bean>
	<bean class="com.jspiders.springcore.bean.AadharBean" name="aadhar1">
	<constructor-arg name="id" value="2"/>
	<constructor-arg name="aadharNo" value="361023561018"/>
	
	</bean>
	
</beans>