<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	
	<bean class="com.jspiders.springcore.bean.StudentBean1" name="Student1">
	<property name="id" value="1"/>
	<property name="name" value="Rutuja"/>
	<property name="Subjects">
	<list>
	<bean class="com.jspiders.springcore.bean.CourseBean" name="Course1">
	<property name="id" value="1"/>
	<property name="name" value="Core Java"/>
	</bean>
	
	</list>
	</property>
	
	
	</bean>
	<bean class="com.jspiders.springcore.bean.StudentBean1" name="Student2">
	<property name="id" value="2"/>
	<property name="name" value="Reshma"/>
	<property name="Subjects">
	<list>
	<bean class="com.jspiders.springcore.bean.CourseBean" name="Course2">
	<property name="id" value="2"/>
	<property name="name" value="Advance Java"/>
	</bean>
	
	</list>
	</property>
	
	
	</bean>
	
	</beans>